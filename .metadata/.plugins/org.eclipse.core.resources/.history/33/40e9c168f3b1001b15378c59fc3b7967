package as_401;

public class Maximum {
	
	public void swap(int a, int b) {
		a = a+b;
		b = a-b;
		a = a-b;
	}
	
	public void sort(Integer[] list) {
		for (int i = 0; i < list.length - 1; i++) {
			for (int j = i + 1; j < list.length; j++) {
				if(list[i] < list[j]) {
					swap(list[i], list[j]);
				}
			}
		}
	}
	
	
	public static void main(String[] args) {
		
	}

}
