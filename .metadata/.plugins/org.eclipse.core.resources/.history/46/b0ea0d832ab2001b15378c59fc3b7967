package demo;

public class PrimitiveDataSample {
	public static void main(String[] args) {
		int number = 2017;
		byte b = 23;
		boolean checked = true;
		float f = 23.5f;
		double d = 34.5;
		long l = 2021;
		
		String dataString = "xxx";
		
		
		int x = 5;
		short n = 4;
		
		n = (short) x;
		
		
		int a = 5, z = 9, c = 7;
		
		c = new Integer(a) + new Integer(z) ;
		
		System.out.println(c);
		

		
	} 
}
